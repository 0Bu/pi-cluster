{
  "extends": [
    "config:recommended",
    "regexManagers:helmChartYamlAppVersions",
    ":semanticCommits"
  ],
  "platform": "github",
  "configMigration": true,
  "automerge": true,
  "bumpVersion": "patch",
  "ignoreTests": true,
  "repositories": [
    "0Bu/rpi-k3s-cluster"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "homebridge/homebridge"
      ],
      "versioning": "loose"
    }
  ]
}
